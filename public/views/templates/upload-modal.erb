<!--Upload Modal-->
  <div class="modal fade" id="uploadModal" tabindex="-1" role="form" aria-labelledby="uploadModalLabel">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
      <form method="POST" action="/upload" enctype='multipart/form-data'>
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title" id="uploadModalLabel">Upload your stupid image.</h4>
        </div>
        <div class="modal-body">
            <div class="form-group">
              <label for="imageTitleInput">Image Title</label>
              <input type="text" class="form-control" id="imageTitleInput" name="filename" placeholder="Give this image a name">
            </div>
            <div class="form-group">
              <label for="imageDescInput">Image Description</label>
              <textarea class="form-control" rows="3" id="imageDescInput" name="fileDesc" placeholder="Tell us a little bit about this image."></textarea>
            </div>
            <div class="form-group">
              <label for="imageUploadInput">File input</label>
              <input type="file" class="btn" id="imageUploadInput" name="file_upload">
              <p class="help-block">Make sure it is something really dumb.</p>
            </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-primary">Upload</button>
        </div>
        </form>
      </div>
    </div>
  </div>